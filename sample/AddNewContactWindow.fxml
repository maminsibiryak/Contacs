<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane
      prefHeight="179.0" prefWidth="176.0"
      xmlns="http://javafx.com/javafx/8.0.121"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="sample.AddNewContactController"
>
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="User name" />
      <Button fx:id="buttonAdd" onAction="#addToDb" layoutX="57.0" layoutY="140.0" mnemonicParsing="false" text="Add to db" />
      <Label layoutX="14.0" layoutY="78.0" text="Phone number" />
      <TextField fx:id="userName" layoutX="14.0" layoutY="31.0" />
      <TextField fx:id="phoneNumber" layoutX="14.0" layoutY="95.0" />
   </children>
</Pane>
